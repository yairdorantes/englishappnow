_count}\n                      state={\n                        post.likes.indexOf(user.user_id) !== -1 ? true : false\n                      }\n                      actions={{\n                        like: () => giveLike(post),\n                        dislike: () => giveDislike(post),\n                      }}\n                    />\n                  </div>\n                </div>\n              );\n            })\n          )}\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default Posts;\n","/home/yair/Escritorio/Desktop/english/src/components/PostPage.js",["145","146","147","148"],[],"import \"./styles/singlePost.css\";\nimport { useContext, useEffect, useState } from \"react\";\nimport { useParams } from \"react-router-dom\";\nimport Loader from \"./Loader\";\nimport AuthContext from \"../context/AuthContext\";\n// import MenuBar from \"./MenuBar\";\nimport ReactMarkdown from \"react-markdown\";\nimport remarkGfm from \"remark-gfm\";\nimport { isMobile } from \"react-device-detect\";\nimport mySite from \"./Domain\";\nimport MenuBar from \"./MenuBar\";\nimport NewMenu from \"./NewMenu\";\n\nconst parametros = {\n  method: \"GET\",\n  headers: {\n    accept: \"application/json\",\n  },\n};\n\nconst PostPage = () => {\n  let { user } = useContext(AuthContext);\n  const [post, setPost] = useState();\n  const params = useParams();\n\n  const fetchAPi = async () => {\n    let url = `${mySite}post/${params.id}`;\n    const response = await fetch(url, parametros);\n    const responseJSON = await response.json();\n    setPost(responseJSON.post[0]);\n  };\n\n  useEffect(() => {\n    fetchAPi();\n  }, []);\n\n  return (\n    <>\n      <NewMenu />\n      <div className=\"container-post-content pb-[100px]\">\n        {!post ? (\n          <Loader></Loader>\n        ) : (\n          <div className=\"post-content\">\n            <h1 className=\"title-post text-4xl font-bold\">{post.title}</h1>\n            <div className=\"box-image-post\">\n              <im